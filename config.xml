        <icon src="LauncherIcons/mipmap-xhdpi/ic_launcher.png" gap:platform="android" gap:density="xhdpi" />
        <icon src="LauncherIcons/mipmap-xxhdpi/ic_launcher.png" gap:platform="android" gap:density="xxhdpi" />
        <icon src="LauncherIcons/mipmap-xxxhdpi/ic_launcher.png" gap:platform="android" gap:density="xxxhdpi" />
